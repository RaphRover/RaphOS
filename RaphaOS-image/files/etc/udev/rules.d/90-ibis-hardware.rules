# Luxonis OAK cameras
SUBSYSTEM=="usb", ATTRS{idVendor}=="03e7", MODE="0666"

# Insta360 cameras
SUBSYSTEM=="usb", ATTRS{idVendor}=="2e1a", ATTRS{idProduct}=="0002", MODE="0666", SYMLINK+="insta%n"

# Haorutech UWB Ultra Wideband Positioning Module
SUBSYSTEM=="tty", ATTRS{idVendor}=="1a86", ATTRS{idProduct}=="7523", SYMLINK+="beacon"

# Externel USB Disk
KERNEL=="sda1", SUBSYSTEM=="block", ACTION=="add", ENV{ID_FS_TYPE}=="exfat", \
  ENV{SYSTEMD_WANTS}="home-ibis-external_disk_mountpoint.mount"